@article{Huybrechs2007asd,
	title = {A {Sparse} {Discretization} for {Integral} {Equation} {Formulations} of {High} {Frequency} {Scattering} {Problems}},
	volume = {29},
	issn = {1064-8275, 1095-7197},
	url = {http://epubs.siam.org/doi/10.1137/060651525},
	doi = {10.1137/060651525},
	abstract = {We consider two-dimensional scattering problems, formulated as an integral equation deﬁned on the boundary of the scattering obstacle. The oscillatory nature of high-frequency scattering problems necessitates a large number of unknowns in classical boundary element methods. In addition, the corresponding discretization matrix of the integral equation is dense. We formulate a boundary element method with basis functions that incorporate the asymptotic behavior of the solution at high frequencies. The method exhibits the eﬀectiveness of asymptotic methods at high frequencies with only few unknowns, but retains accuracy for lower frequencies. New in our approach is that we combine this hybrid method with very eﬀective quadrature rules for oscillatory integrals. As a result, we obtain a sparse discretization matrix for the oscillatory problem. Moreover, numerical experiments indicate that the accuracy of the solution actually increases with increasing frequency. The sparse discretization applies to problems where the phase of the solution can be predicted a priori, for example in the case of smooth and convex scatterers.},
	language = {en},
	number = {6},
	urldate = {2022-10-01},
	journal = {SIAM Journal on Scientific Computing},
	author = {Huybrechs, Daan and Vandewalle, Stefan},
	month = jan,
	year = {2007},
	pages = {2305--2328},
}

