	year = {2016},
	note = {Place: NEW YORK
Publisher: Elsevier Inc
tex.copyright: 2015},
	keywords = {Algorithms, Buildings and facilities, Cyber swarm algorithm, Electricity demand, Engineering, Interdisciplinary Applications, Mathematics, Mechanics, Micro-siting, Models, Multidisciplinary, Physical Sciences, Risk management, Science \& Technology, Technology, Wind farm, Wind power},
	pages = {2177--2189},
}

