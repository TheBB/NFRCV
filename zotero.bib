@article{Pegolotti2019iao,
	title = {Isogeometric {Analysis} of the electrophysiology in the human heart: {Numerical} simulation of the bidomain equations on the atria},
	volume = {343},
	issn = {00457825},
	shorttitle = {Isogeometric {Analysis} of the electrophysiology in the human heart},
