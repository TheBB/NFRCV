@article{keuchel_evaluation_2017,
	title = {Evaluation of hypersingular and nearly singular integrals in the {Isogeometric} {Boundary} {Element} {Method} for acoustics},
	volume = {325},
	issn = {0045-7825},
	url = {http://www.sciencedirect.com/science/article/pii/S0045782517305704},
	doi = {10.1016/j.cma.2017.07.025},
	abstract = {Special integration routines for hypersingular and nearly singular integrals that arise in an Isogeometric Boundary Element Method (IGABEM) are presented. The IGABEM is applied to an acoustic problem in the frequency domain that corresponds to the Helmholtz equation in three dimensions. A major topic of such an acoustical BEM is the non-uniqueness of the conventional boundary integral equation, which can be corrected by the Burton–Miller formulation at the cost of a hypersingular integral that requires the presented routines. The combination of the BEM with an Isogeometric Analysis (IGA) is advantageous, since the exact CAD description is directly incorporated to the numerical simulation. As the geometry is represented exactly, the integration routines become even more relevant and cannot be masked by the geometrical approximation. For the conventional approximation with plane Lagrange elements, a variety of different integration routines exist, but are not investigated in the scope of an IGA. Therefore, the hypersingular integration scheme of Guiggiani is adapted to be applicable to this new methodology. A direct evaluation of the hypersingular integral is possible, instead of an integration over the complete surface, as in the often used regularized boundary integral equation. Additionally, the sinh-transformation for nearly singular integrals is introduced to achieve more accurate results with the same number of integration points as the conventional Gaussian integration. The new formulation is analyzed under the special case of the oscillatory kernels in acoustics.},
	language = {en},
	urldate = {2022-10-01},
	journal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Keuchel, Sören and Hagelstein, Nils Christian and Zaleski, Olgierd and von Estorff, Otto},
	month = oct,
	year = {2017},
	note = {Publisher: Elsevier BV
tex.owner: TK$_{\textrm{C}}$M
tex.timestamp: 2019-04-11},
	keywords = {Boundary Element Method, Helmholtz equation, Isogeometric analysis, Numerical integration},
	pages = {488--504},
}

